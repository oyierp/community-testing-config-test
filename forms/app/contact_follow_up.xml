      <bind nodeset="/contact_follow_up/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/contact_follow_up/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /contact_follow_up/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/contact_follow_up/patient_age_in_months" type="string" calculate="difference-in-months(  /contact_follow_up/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/contact_follow_up/patient_age_display" type="string" calculate="concat(
 concat( /contact_follow_up/patient_age_in_years , if( /contact_follow_up/patient_age_in_years =1,' year and ',' years and ')),
 concat( /contact_follow_up/patient_age_in_months  mod 12, if(( /contact_follow_up/patient_age_in_months  mod 12)=1,' month old',' months old'))
)"/>
      <bind nodeset="/contact_follow_up/needs_follow_up" type="string" calculate="if( /contact_follow_up/group_follow_up/needs_referral  = 'true', 'no', 'yes')"/>
      <bind nodeset="/contact_follow_up/follow_up_count" type="string" calculate="if(../inputs/t_follow_up_count != '', ../inputs/t_follow_up_count, 1)"/>
      <bind nodeset="/contact_follow_up/has_symptoms" type="string" calculate="if( /contact_follow_up/group_follow_up/cough  = 'yes' or  /contact_follow_up/group_follow_up/fever  = 'yes' or  /contact_follow_up/group_follow_up/difficulty_breathing  = 'yes' or  /contact_follow_up/group_follow_up/temperature  &gt;=38, 'true', 'false')"/>
      <bind nodeset="/contact_follow_up/has_symptoms_only" type="string" calculate="if( /contact_follow_up/group_follow_up/cough  = 'yes' or  /contact_follow_up/group_follow_up/fever  = 'yes' or  /contact_follow_up/group_follow_up/difficulty_breathing  = 'yes', 'true', 'false')"/>
      <bind nodeset="/contact_follow_up/last_contact_date" type="string" calculate="2020-08-07"/>
      <bind nodeset="/contact_follow_up/group_follow_up/n_details" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/group_follow_up/date_last_contact" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/group_follow_up/follow_up_type" type="select1" jr:constraintMsg="There's no registered phone number" required="true()" constraint="not(selected(.,'phone') and  /contact_follow_up/inputs/contact/phone  = '')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/call_button" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/follow_up_type ='phone' and  /contact_follow_up/inputs/contact/phone !=''"/>
      <bind nodeset="/contact_follow_up/group_follow_up/no_phone" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/follow_up_type ='phone' and  /contact_follow_up/inputs/contact/phone =''"/>
      <bind nodeset="/contact_follow_up/group_follow_up/status_call" type="select1" relevant=" /contact_follow_up/group_follow_up/follow_up_type ='phone' and  /contact_follow_up/inputs/contact/phone !=''" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/status_call_other" type="string" relevant=" /contact_follow_up/group_follow_up/status_call  = 'other'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/status_visit" type="select1" relevant=" /contact_follow_up/group_follow_up/follow_up_type  = 'in_person'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/status_visit_other" type="string" relevant=" /contact_follow_up/group_follow_up/status_visit  = 'other'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/is_available" type="string" calculate="if(( /contact_follow_up/group_follow_up/symptoms !='' and  /contact_follow_up/group_follow_up/symptoms !='deceased') or ( /contact_follow_up/group_follow_up/status_call  != '' and  /contact_follow_up/group_follow_up/status_call  != 'wrong_number' and  /contact_follow_up/group_follow_up/status_call  != 'not_responding' and  /contact_follow_up/group_follow_up/status_call  != 'call_not_going_through') or ( /contact_follow_up/group_follow_up/status_visit  != '' and  /contact_follow_up/group_follow_up/status_visit  !='moved_away' and  /contact_follow_up/group_follow_up/status_visit  !='not_found_in_usual_place'), 'yes','no')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/symptoms" type="select1" relevant=" /contact_follow_up/group_follow_up/status_visit  = 'available' or  /contact_follow_up/group_follow_up/status_visit  = 'not_found_in_usual_place' or  /contact_follow_up/group_follow_up/status_call  = 'responded'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/date_of_death" type="date" relevant="selected( /contact_follow_up/group_follow_up/symptoms , 'deceased')" jr:constraintMsg="Date must not be in the future" required="true()" constraint=". &lt; today()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/developed_symptoms" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/temperature" type="decimal" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" jr:constraintMsg="Temperature should be between 25 and 43 inclusive" constraint=".&gt;=25 and .&lt;=43"/>
      <bind nodeset="/contact_follow_up/group_follow_up/c_temperature" type="string" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" calculate="if( /contact_follow_up/group_follow_up/temperature != '',  /contact_follow_up/group_follow_up/temperature , '')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/n_fever" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/temperature &gt;=38 and  /contact_follow_up/group_follow_up/symptoms ='symptomatic'"/>
      <bind nodeset="/contact_follow_up/group_follow_up/n_not_fever" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/temperature !='' and  /contact_follow_up/group_follow_up/temperature &lt;38 and  /contact_follow_up/group_follow_up/temperature !=0 and  /contact_follow_up/group_follow_up/symptoms ='symptomatic'"/>
      <bind nodeset="/contact_follow_up/group_follow_up/fever" type="select1" relevant="( /contact_follow_up/group_follow_up/temperature ='' or  /contact_follow_up/group_follow_up/temperature =0) and  /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/c_fever" type="string" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" calculate="if( /contact_follow_up/group_follow_up/temperature &gt;=38 or  /contact_follow_up/group_follow_up/fever  = 'yes', 'yes', 'no')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/cough" type="select1" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/running_nose" type="select1" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/sore_throat" type="select1" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/difficulty_breathing" type="select1" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'" required="true()"/>
      <bind nodeset="/contact_follow_up/group_follow_up/other_symptoms" type="string" relevant=" /contact_follow_up/group_follow_up/symptoms ='symptomatic'"/>
      <bind nodeset="/contact_follow_up/group_follow_up/needs_referral" type="string" calculate="if( /contact_follow_up/group_follow_up/fever  = 'yes' or  /contact_follow_up/group_follow_up/cough  = 'yes' or  /contact_follow_up/group_follow_up/running_nose  = 'yes' or  /contact_follow_up/group_follow_up/sore_throat  = 'yes' or  /contact_follow_up/group_follow_up/difficulty_breathing  = 'yes', 'true','false')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/referral_snippet" type="string" calculate="if( /contact_follow_up/group_follow_up/needs_referral  = 'true',' and refer the patient to hospital', '')"/>
      <bind nodeset="/contact_follow_up/group_follow_up/is_not_available_reason" type="string" calculate="if( /contact_follow_up/group_follow_up/symptoms  = 'deceased', 'deceased', if( /contact_follow_up/group_follow_up/status_call  = 'wrong_number' or  /contact_follow_up/group_follow_up/status_call  = 'not_responding' or  /contact_follow_up/group_follow_up/status_call  = 'call_not_going_through', 'phone_off', if( /contact_follow_up/group_follow_up/status_visit  = 'moved_away', 'moved_away', if( /contact_follow_up/group_follow_up/status_visit  = 'not_found_in_usual_place', 'person_not_found', '' ))))"/>
      <bind nodeset="/contact_follow_up/group_follow_up/is_not_available_reason_other" type="string"/>
      <bind nodeset="/contact_follow_up/group_follow_up/r_referral" readonly="true()" type="string" relevant=" /contact_follow_up/has_symptoms ='true'"/>
      <bind nodeset="/contact_follow_up/r_summary/r_submit" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/r_summary" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/r_patient_info" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/r_screening" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/c_condition" type="string" calculate="if( /contact_follow_up/has_symptoms ='true', if(selected( /contact_follow_up/group_follow_up/symptoms ,'deceased'),' /contact_follow_up/inputs/contact/name  passed away','Has Acute Respiratory infection symptoms'),'No Acute Respiratory infection symptoms')"/>
      <bind nodeset="/contact_follow_up/r_summary/r_no_condition" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/r_followup" readonly="true()" type="string"/>
      <bind nodeset="/contact_follow_up/r_summary/n_notify" readonly="true()" type="string" relevant=" /contact_follow_up/has_symptoms ='true' and not(selected( /contact_follow_up/group_follow_up/symptoms ,'deceased'))"/>
      <bind nodeset="/contact_follow_up/r_summary/n_notify2" readonly="true()" type="string" relevant="selected( /contact_follow_up/group_follow_up/symptoms ,'deceased')"/>
      <bind nodeset="/contact_follow_up/r_summary/r_followup_note" readonly="true()" type="string" relevant=" /contact_follow_up/group_follow_up/status_call ='responded' and not(selected( /contact_follow_up/group_follow_up/symptoms ,'deceased'))"/>
      <bind nodeset="/contact_follow_up/r_summary/r_followup_note_14" readonly="true()" type="string" relevant=" /contact_follow_up/inputs/t_follow_up_count ='14' or selected( /contact_follow_up/group_follow_up/symptoms ,'deceased')"/>
      <bind nodeset="/contact_follow_up/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/contact_follow_up/inputs">
      <label>Patient</label>
      <group ref="/contact_follow_up/inputs/contact">
        <label>Contact</label>
        <input appearance="db-object" ref="/contact_follow_up/inputs/contact/_id">
          <label>What is the traveller's name?</label>
          <hint>Select a person from list</hint>
        </input>
        <group ref="/contact_follow_up/inputs/contact/parent"/>
      </group>
    </group>
    <group appearance="field-list" ref="/contact_follow_up/group_follow_up">
      <label>Contact details</label>
      <input ref="/contact_follow_up/group_follow_up/n_details">
        <label>**Name:** <output value=" /contact_follow_up/inputs/contact/name "/> **Date of birth:** <output value=" /contact_follow_up/inputs/contact/date_of_birth "/> **Phone number:** <output value=" /contact_follow_up/inputs/contact/phone "/></label></input>
      <input ref="/contact_follow_up/group_follow_up/date_last_contact">
        <label>**Date of last contact:** <output value=" /contact_follow_up/inputs/contact/date_of_last_contact "/></label>
      </input>
      <select1 ref="/contact_follow_up/group_follow_up/follow_up_type">
        <label>How are you following up?</label>
        <item>
          <label ref="jr:itext('/contact_follow_up/group_follow_up/follow_up_type/in_person:label')"/>
          <value>in_person</value>
        </item>
        <item>
          <label ref="jr:itext('/contact_follow_up/group_follow_up/follow_up_type/phone:label')"/>
          <value>phone</value>
        </item>
      </select1>
      <input ref="/contact_follow_up/group_follow_up/call_button">
